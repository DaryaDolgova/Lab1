#include <iostream>
#include <cmath>
using namespace std;
int main ()
{
double a,b,a2,b2,s,p,r,ch;
cout<<"a= "; cin>>a;
cout<<"b= "; cin>>b;
a2= abs(a);
b2= abs(b);
s=a2+b2;
p=a2*b2;
r=a2-b2;
ch=a2/b2;
cout<<"s= "<<s<<endl;
cout<<"p= "<<p<<endl;
cout<<"r= "<<r<<endl;
cout<<"ch= "<<ch<<endl;
return 0;
}
